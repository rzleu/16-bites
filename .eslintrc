{
  "plugins": ["jsx-a11y"],
  "extends": ["airbnb", "airbnb/hooks"],
  "parser": "babel-eslint",
  "rules": {
    "quotes": [1, "single", { "avoidEscape": true }],
    "jsx-quotes": ["error", "prefer-single"],
    "jsx-a11y/label-has-associated-control": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ],
    "jsx-a11y/label-has-for": [
      "error",
      {
        "required": {
          "some": ["nesting", "id"]
        }
      }
    ]
  }
}
